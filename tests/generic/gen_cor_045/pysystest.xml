<?xml version="1.0" encoding="utf-8"?>
<pysystest type="auto">

    <description>
        <title>Game: play a sample guessing game</title>
        <purpose><![CDATA[
Plays an example guessing game where the user approves a token to allow the game to spend money on it's behalf. Mimics
the number guessing game in the samples repository for local testing on an arbitrary network.
]]>
        </purpose>
    </description>

    <classification>
        <groups inherit="true">>
        </groups>
        <modes inherit="true">
            <mode>obscuro.sepolia</mode>
            <mode>obscuro.uat</mode>
            <mode>obscuro.dev</mode>
            <mode>obscuro.local</mode>
            <mode>arbitrum.sepolia</mode>
            <mode>goerli</mode>
            <mode>ganache</mode>
            <mode>sepolia</mode>
        </modes>
    </classification>

    <data>
        <class name="PySysTest" module="run"/>
    </data>

    <traceability>
        <requirements>
            <requirement id=""/>
        </requirements>
    </traceability>
</pysystest>
