<?xml version="1.0" encoding="utf-8"?>
<pysystest type="auto">

    <description>
        <title>Callback: ethers client using provider on "block" and then off (newHeads)</title>
        <purpose><![CDATA[
Checks that we can use ethers provider once to receive newHeads, and then remove the subscription once a certain number
of blocks are received.
]]>
        </purpose>
    </description>

    <classification>
        <groups inherit="true">
            <group>ethers</group>
        </groups>
        <modes inherit="true">
            <mode>ten.sepolia</mode>
            <mode>ten.uat</mode>
            <mode>ten.dev</mode>
            <mode>ten.local</mode>
            <mode>ten.sim</mode>
            <mode>arbitrum.sepolia</mode>
            <mode>ganache</mode>
            <mode>sepolia</mode>
        </modes>
    </classification>

    <data>
        <class name="PySysTest" module="run"/>
    </data>

    <traceability>
        <requirements>
            <requirement id=""/>
        </requirements>
    </traceability>
</pysystest>
