<?xml version="1.0" encoding="utf-8"?>
<pysystest type="auto">

    <description>
        <title>Access List - one contracts calls another with and without using access lists</title>
        <purpose><![CDATA[
Contact B makes a call to contract A to set a value, with and without the access list specified in the transaction.
The gas used with an access list should be less than the gas used without it. 
]]>
        </purpose>
    </description>

    <classification>
        <groups inherit="true">
            <group>skip</group>
        </groups>
        <modes inherit="true">
            <mode>ten.sepolia</mode>
            <mode>ten.uat</mode>
            <mode>ten.dev</mode>
            <mode>ten.local</mode>
            <mode>arbitrum.sepolia</mode>
            <mode>ganache</mode>
            <mode>sepolia</mode>
        </modes>
    </classification>

    <data>
        <class name="PySysTest" module="run"/>
    </data>

    <traceability>
        <requirements>
            <requirement id=""/>
        </requirements>
    </traceability>
</pysystest>
