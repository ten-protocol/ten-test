<?xml version="1.0" encoding="utf-8"?>
<pysystest type="auto">

    <description>
        <title>Events: transact and check transaction receipt event log</title>
        <purpose><![CDATA[
Deploy the storage contract, call to get the current value, store a new value as a transaction, extract the
value from the transaction log, and call again.
]]>
        </purpose>
    </description>

    <classification>
        <groups inherit="true">
            <group>events</group>
        </groups>
        <modes inherit="true">
            <mode>obscuro.dev</mode>
            <mode>obscuro.local</mode>
            <mode>goerli</mode>
            <mode>ganache</mode>
        </modes>
    </classification>

    <data>
        <class name="PySysTest" module="run"/>
    </data>

    <traceability>
        <requirements>
            <requirement id=""/>
        </requirements>
    </traceability>
</pysystest>
