<?xml version="1.0" encoding="utf-8"?>
<pysystest type="auto">

    <description>
        <title>Visibility Config: open up calls to storageAt</title>
        <purpose><![CDATA[
Uses the simple guessing game but opens it up to expose and allow calls to get_storage. Thus the user is now able to
determine the hidden number and make a single call to win.
]]>
        </purpose>
    </description>

    <classification>
        <groups inherit="true">
            <group>hardhat</group>
        </groups>
        <modes inherit="true">
            <mode>ten.sepolia</mode>
            <mode>ten.uat</mode>
            <mode>ten.dev</mode>
            <mode>ten.local</mode>
            <mode>ten.sim</mode>
        </modes>
    </classification>

    <data>
        <class name="PySysTest" module="run"/>
    </data>

    <traceability>
        <requirements>
            <requirement id=""/>
        </requirements>
    </traceability>
</pysystest>
