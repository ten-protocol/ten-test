<?xml version="1.0" encoding="utf-8"?>
<pysystest type="auto">

    <description>
        <title>Bridge: send native funds and tokens in single xchain message</title>
        <purpose><![CDATA[
Attempts to perform both ERC20 token withdrawals and native funds withdrawal in the same xchain message, and validates
that the withdrawals take place.
]]>
        </purpose>
    </description>

    <classification>
        <groups inherit="true">
            <group>bridge</group>
            <group>withdrawals</group>
        </groups>
        <modes inherit="true">
            <mode>ten.sepolia</mode>
            <mode>ten.uat</mode>
            <mode>ten.dev</mode>
            <mode>ten.local</mode>
        </modes>
    </classification>

    <data>
        <class name="PySysTest" module="run"/>
    </data>

    <traceability>
        <requirements>
            <requirement id=""/>
        </requirements>
    </traceability>
</pysystest>
