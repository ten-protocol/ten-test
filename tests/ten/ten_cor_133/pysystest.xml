<?xml version="1.0" encoding="utf-8"?>
<pysystest type="auto">

    <description>
        <title>System Calls: transact and emit an event with the tx timestamp</title>
        <purpose><![CDATA[
Compares the sent time with the timestamp returned from the systems call contract and ensure it is less than
a given value over a set number of transactions.
]]>
        </purpose>
    </description>

    <classification>
        <groups inherit="true">
            <group>system_calls</group>
        </groups>
        <modes inherit="true">
            <mode>ten.sepolia</mode>
            <mode>ten.uat</mode>
            <mode>ten.dev</mode>
            <mode>ten.local</mode>
            <mode>ten.sim</mode>
        </modes>
    </classification>

    <data>
        <class name="PySysTest" module="run"/>
    </data>

    <traceability>
        <requirements>
            <requirement id=""/>
        </requirements>
    </traceability>
</pysystest>
