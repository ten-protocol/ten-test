<?xml version="1.0" encoding="utf-8"?>
<pysystest type="auto">

    <description>
        <title>RPC - rates for handling balance requests, increasing clients</title>
        <purpose><![CDATA[
Spawns a set number of clients querying for their balance, and measures the latency in the response, the client
throughput and the bulk throughput across all clients. 
]]>
        </purpose>
    </description>

    <classification>
        <groups inherit="true">
            <group>performance</group>
        </groups>
        <modes inherit="true">
            <mode>ten.sepolia</mode>
            <mode>ten.uat</mode>
            <mode>ten.dev</mode>
            <mode>ten.local</mode>
            <mode>ten.sim</mode>
        </modes>
    </classification>

    <data>
        <class name="PySysTest" module="run"/>
    </data>

    <traceability>
        <requirements>
            <requirement id=""/>
        </requirements>
    </traceability>
</pysystest>
