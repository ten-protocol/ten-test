set terminal pdf enhanced size 18cm, 18cm
set output ARG2."_all_clients.pdf"
set multiplot layout 2,2 rowsfirst title ARG2.": ".ARG1 noenhanced scale 0.95,0.95
set grid
set lmargin 10
set bmargin 3
set key font 'Courier,10'
set key right top
set yrange [*<0:*]
set xtics font "Courier,11"
set ytics nomirror font "Courier,11"
set label "{/Courier:Bold=13 Branch}: ".ARG3 left at screen 0.59, screen 0.925
set label "{/Courier:Bold=13 Date}: ".ARG4 left at screen 0.59, screen 0.900
set label "{/Courier:Bold=13 Environment}: ".ARG5 left at screen 0.59, screen 0.875
set label "{/Courier:Bold=13 Throughput}: ".ARG6." transactions/s" left at screen 0.59, screen 0.850

#plot 1
set size 0.5, 0.45
set origin 0.0, 0.50
set style fill solid 0.5
set title "{/Arial:Bold=13 Bulk throughput with varying number of subscribers}"
set xlabel "Number of Subscribers"  font "Courier,12"
set ylabel "Bulk throughput (e/s)" font "Courier,12" offset 0,0
plot "results.log" using 1:2 with linespoints title "Bulk throughput"

