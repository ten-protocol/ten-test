<?xml version="1.0" encoding="utf-8"?>
<pysystest type="auto">

    <description>
        <title>Session keys: delete a session key after transacting</title>
        <purpose><![CDATA[
Creates and funds a session key, then transacts through it. After this tries to delete it without deactivating,
delete after deactivating, transact after deleting, and activate after deleting.
]]>
        </purpose>
    </description>

    <classification>
        <groups inherit="true">
            <group>session_keys</group>
        </groups>
        <modes inherit="true">
            <mode>ten.sepolia</mode>
            <mode>ten.uat</mode>
            <mode>ten.dev</mode>
            <mode>ten.local</mode>
            <mode>ten.sim</mode>
        </modes>
    </classification>

    <data>
        <class name="PySysTest" module="run"/>
    </data>

    <traceability>
        <requirements>
            <requirement id=""/>
        </requirements>
    </traceability>
</pysystest>
