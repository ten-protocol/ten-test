<?xml version="1.0" encoding="utf-8"?>
<pysystest type="auto">

    <description>
        <title>Tenscan RPC - check transaction and contract counts</title>
        <purpose><![CDATA[
Checks the calls to get the total number of contracts and transactions via the scan_interface return the expected
results before and after deploying a contract and interacting with it.
]]>
        </purpose>
    </description>

    <classification>
        <groups inherit="true">
            <group>tenscan</group>
        </groups>
        <modes inherit="true">
            <mode>ten.sepolia</mode>
            <mode>ten.uat</mode>
            <mode>ten.dev</mode>
            <mode>ten.local</mode>
        </modes>
    </classification>

    <data>
        <class name="PySysTest" module="run"/>
    </data>

    <traceability>
        <requirements>
            <requirement id=""/>
        </requirements>
    </traceability>
</pysystest>
