<?xml version="1.0" encoding="utf-8"?>
<pysystest type="auto">

    <description>
        <title>Account going through separate connection can't get receipt or logs </title>
        <purpose><![CDATA[
If an event is not visibility to everyone, then an account sharing a token should be able to still get it, but an
account using a different token should not.
]]>
        </purpose>
    </description>

    <classification>
        <groups inherit="true">
        </groups>
        <modes inherit="true">
            <mode>ten.sepolia</mode>
            <mode>ten.uat</mode>
            <mode>ten.dev</mode>
            <mode>ten.local</mode>
            <mode>ten.sim</mode>
        </modes>
    </classification>

    <data>
        <class name="PySysTest" module="run"/>
    </data>

    <traceability>
        <requirements>
            <requirement id=""/>
        </requirements>
    </traceability>
</pysystest>
