          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  test, \n  COUNT(CASE WHEN outcome IN ('FAILED', 'TIMEDOUT', 'DUMPEDCORE', 'BLOCKED') THEN 1 END) AS num_fails,\n  COUNT(*) AS total_runs\nFROM results_outcome\nWHERE environment = '${environment}'\nGROUP BY test\nHAVING COUNT(CASE WHEN outcome IN ('FAILED', 'TIMEDOUT', 'DUMPEDCORE', 'BLOCKED') THEN 1 END) > 0\nORDER BY num_fails DESC;\n\n",
