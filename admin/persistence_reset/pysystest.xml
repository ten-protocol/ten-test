<?xml version="1.0" encoding="utf-8"?>
<pysystest type="auto">

    <description>
        <title>Reset entries to the latest transaction count</title>
        <purpose><![CDATA[
If the latest transaction count is greater than the persisted nonce, e.g. if we are pointing a new clone of the
test repository against an existing installation, then reset the persisted nonce to the count. 
]]>
        </purpose>
    </description>

    <classification>
        <groups inherit="true">
            <group></group>
        </groups>
        <modes inherit="true">
            <mode>obscuro.sepolia</mode>
            <mode>obscuro.uat</mode>
            <mode>obscuro.dev</mode>
            <mode>obscuro.local</mode>
            <mode>arbitrum.sepolia</mode>
            <mode>sepolia</mode>
        </modes>
    </classification>

    <data>
        <class name="PySysTest" module="run"/>
    </data>

    <traceability>
        <requirements>
            <requirement id=""/>
        </requirements>
    </traceability>
</pysystest>
