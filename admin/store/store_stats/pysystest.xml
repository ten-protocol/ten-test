<?xml version="1.0" encoding="utf-8"?>
<pysystest type="auto">

    <description>
        <title>Persist stats for a range of metrics on a ten network</title>
        <purpose><![CDATA[
Run to persist into external storage L1 bridge balance, faucet balance, total transaction counts, and total number
of deployed contracts.
]]>
        </purpose>
    </description>

    <classification>
        <groups inherit="true">
        </groups>
        <modes inherit="true">
            <mode>ten.sepolia</mode>
            <mode>ten.uat</mode>
            <mode>ten.local</mode>
        </modes>
    </classification>

    <data>
        <class name="PySysTest" module="run"/>
    </data>

    <traceability>
        <requirements>
            <requirement id=""/>
        </requirements>
    </traceability>
</pysystest>
