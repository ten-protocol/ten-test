<?xml version="1.0" encoding="utf-8"?>
<pysystest type="auto">

    <description>
        <title>Check the batch production rate on sepolia-testnet</title>
        <purpose><![CDATA[
Logs out the transaction and batch count every X interval seconds, and after an elapsed duration checks that the
increase is not greater than a threshold value.
]]>
        </purpose>
    </description>
    <classification>
        <groups inherit="true">
            <group>health</group>
        </groups>
        <modes inherit="true">
            <mode>ten.sepolia</mode>
            <mode>ten.uat</mode>
            <mode>ten.dev</mode>
            <mode>ten.local</mode>
        </modes>
    </classification>

    <data>
        <class name="PySysTest" module="run"/>
    </data>

    <traceability>
        <requirements>
            <requirement id=""/>
        </requirements>
    </traceability>
</pysystest>
