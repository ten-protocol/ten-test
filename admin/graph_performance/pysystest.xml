<?xml version="1.0" encoding="utf-8"?>
<pysystest type="auto">

    <description>
        <title>Graph performance summary</title>
        <purpose><![CDATA[
Produces an overall summary of the performance metrics overtime for a particular environment.
]]>
        </purpose>
    </description>
    <classification>
        <groups inherit="true">
            <group>performance</group>
        </groups>
        <modes inherit="true">
            <mode>ten.uat</mode>
            <mode>ten.dev</mode>
            <mode>ten.local</mode>
        </modes>
    </classification>

    <data>
        <class name="PySysTest" module="run"/>
    </data>

    <traceability>
        <requirements>
            <requirement id=""/>
        </requirements>
    </traceability>
</pysystest>
